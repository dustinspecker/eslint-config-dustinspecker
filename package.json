{
  "name": "eslint-config-dustinspecker",
  "version": "0.0.0-semantic-release",
  "description": "ESLint shareable config of Dustin Specker's preferences",
  "main": "index.js",
  "scripts": {
    "compile": "babel src --out-dir ./",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "eslint-find-rules": "eslint-find-rules -u ./jsx.js",
    "lint": "eslint ./",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "npm run lint && npm run compile && nyc ava --require babel-register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dustinspecker/eslint-config-dustinspecker.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "author": "Dustin Specker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dustinspecker/eslint-config-dustinspecker/issues"
  },
  "homepage": "https://github.com/dustinspecker/eslint-config-dustinspecker#readme",
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.15.1",
    "babel-cli": "^6.4.0",
    "babel-preset-es2015": "^6.0.12",
    "babel-register": "^6.3.13",
    "coveralls": "^2.11.2",
    "cz-conventional-changelog": "^1.1.5",
    "eslint": "^3.2.2",
    "eslint-find-rules": "^1.1.0",
    "eslint-plugin-ava": "^2.2.1",
    "eslint-plugin-new-with-error": "^1.1.0",
    "eslint-plugin-no-use-extend-native": "^0.3.4",
    "eslint-plugin-react": "^5.0.1",
    "eslint-plugin-xo": "^0.4.0",
    "is-plain-obj": "^1.0.0",
    "nyc": "^6.0.0",
    "semantic-release": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
