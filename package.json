{
  "name": "eslint-config-dustinspecker",
  "version": "0.4.1",
  "description": "ESLint shareable config of Dustin Specker's preferences",
  "main": "index.js",
  "scripts": {
    "compile": "babel src --out-dir ./",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint --config ./src/esnext.js ./",
    "test": "npm run lint && npm run compile && nyc ava --require babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dustinspecker/eslint-config-dustinspecker.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "author": "Dustin Specker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dustinspecker/eslint-config-dustinspecker/issues"
  },
  "homepage": "https://github.com/dustinspecker/eslint-config-dustinspecker#readme",
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.13.0",
    "babel-cli": "^6.4.0",
    "babel-preset-es2015": "^6.0.12",
    "babel-register": "^6.3.13",
    "coveralls": "^2.11.2",
    "eslint": "^2.5.0",
    "eslint-plugin-new-with-error": "^1.1.0",
    "eslint-plugin-no-use-extend-native": "^0.3.1",
    "is-plain-obj": "^1.0.0",
    "nyc": "^6.0.0"
  }
}
